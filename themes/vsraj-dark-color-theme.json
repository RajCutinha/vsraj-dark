{
  "name": "vsraj-dark",
  "type": "dark",
  "colors": {
    // Default Colors ---
    "editor.background": "#101010",
    "editor.foreground": "#E2D5C2",
    "editorGroup.border": "#ffffff",
    "editorGroupHeader.tabsBackground": "#0F111A",
    "editorLineNumber.foreground": "#4739A1",
    // "editorLineNumber.activeForeground": "#8D94ED",
    "editorLineNumber.activeForeground": "#C792EA",
    "editor.lineHighlightBackground": "#ffffff0f",
    "editorGutter.background": "#0F111A",
    "editorOverviewRuler.background": "#161616",
    // Side Bar ---
    "activityBarBadge.background": "#4739A1",
    "activityBar.foreground": "#E2D5C2",
    "activityBar.inactiveForeground": "#e2d5c27f",
    "activityBar.background": "#141414",
    "activityBar.activeBorder": "#4739A1",
    "activityBar.activeFocusBorder": "#4739A1",
    // Button Colors ---
    "button.background": "#20284c",
    // Color Explorer ---
    "sideBar.foreground": "#E2D5C2",
    "sideBar.background": "#111",
    // "sideBarTitle.foreground": "#ce7967",
    "sideBarTitle.foreground": "#C792EA",
    // "sideBarSectionHeader.background": "#1e1e1e",
    "sideBarSectionHeader.background": "#1a1a1a",
    // Color Bottom Panel ---
    "statusBar.background": "#111",
    "statusBar.foreground": "#E2D5C2",
    "statusBar.noFolderBackground": "#111",
    // Tab Colors ---
    "tab.inactiveBackground": "#1E1E1E",
    "tab.border": "#0F111A",
    // "tab.activeBorder": "#4739A1",
    "tab.activeBorder": "#C792EA",
    // "tab.activeForeground": "#8D94ED",
    "tab.activeForeground": "#C792EA",
    "tab.inactiveForeground": "#c4c4c4",
    "tab.activeBackground": "#151c3a85",
    "tab.unfocusedActiveForeground": "#8D94ED",
    "tab.unfocusedActiveBackground": "#151c3a85",
    "tab.unfocusedInactiveForeground": "#c4c4c4",
    "tab.unfocusedInactiveBackground": "#1e1e1e",
    "tab.activeBorderTop": "#1e1e1e",
    // Menu Bar
    "menu.background": "#141414",
    "menu.foreground": "#E2D5C2",
    // Notification
    "notifications.background": "#141414",
    "notifications.foreground": "#E2D5C2",
    "notificationCenter.border": "#4739A1",
    // Title Bar ---
    "titleBar.activeBackground": "#141414",
    "titleBar.inactiveBackground": "#141414",
    "titleBar.border": "#000",
    "titleBar.activeForeground": "#E2D5C2",
    "titleBar.inactiveForeground": "#E2D5C2",
    // Terminal ---
    "terminal.background": "#141414",
    "terminal.border": "#141414",
    "terminal.selectionBackground": "#141414",
    "terminal.foreground": "#fff",
    // Terminal Panel
    "panel.background": "#141414",
    "panelTitle.activeBorder": "#4739A1",
    "panelTitle.activeForeground": "#8D94ED",
    // Nesting ---
    "breadcrumb.background": "#0F111A",
    "breadcrumb.foreground": "#E2D5C2",
    "breadcrumb.activeSelectionForeground": "#C792EA",
    "breadcrumb.focusForeground": "#C792EA",
    "breadcrumbPicker.background": "#0F111A",
    // List and trees
    "list.hoverBackground": "#222",
    "list.hoverForeground": "#8D94ED",
    "list.activeSelectionForeground": "#8D94ED",
    "list.inactiveSelectionForeground": "#8D94ED",
    "list.activeSelectionBackground": "#1e1e1e",
    "list.inactiveFocusBackground": "#1e1e1e",
    "list.inactiveSelectionBackground": "#1e1e1e",
    "list.focusOutline": "#1e1e1e",
    "list.focusBackground": "#1e1e1e",
    // Quick Picker
    "quickInput.background": "#111",
    "quickInputList.focusBackground": "#1e1e1e",
    "quickInput.foreground": "#E2D5C2",
    "quickInputTitle.background": "#8D94ED"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#577a54"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "string constant.other.placeholder",
        "meta.object-literal.key.js"
      ],
      "settings": {
        "foreground": "#FABA5F"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        // "foreground": "#ffffff"
        "foreground": "#0e5c3a"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        // "foreground": "#89DDFF"
        // "foreground": "#FABA5F",
        // "foreground": "#F77F00"
        // "foreground": "#C792EA"
        // "foreground": "#8D94ED"
        "foreground": "#4d67db"
      }
    },
    {
      "name": "Statements",
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "italic",
        // "foreground": "#428cb9",
        // "foreground": "#D09176"
        "foreground": "#ec8052"
      }
    },
    {
      "name": "JSX Text",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        // "foreground": "#f07178"
        // "foreground": "#FABA5F"
        // "foreground": "#C792EA"
        // "foreground": "#F77F00"
        "foreground": "#8D94ED"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        // "foreground": "#82AAFF"
        "foreground": "#8D94ED"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link",
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        // "foreground": "#F78C6C"
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        // "foreground": "#E2D5C2",
        "foreground": "#4aa1e4"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "string.quoted.double.html",
        "meta.tag.attributes.js.jsx",
        "meta.tag.js.jsx",
        "meta.import.js.jsx string",
        "meta.import.js string"
      ],
      "settings": {
        // "foreground": "#C3E88D"
        "foreground": "#daf2b6"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#E2D5C2"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        // "foreground": "#FFCB6B"
        "foreground": "#daf2b6"
      }
    },
    {
      "name": "SubClass",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#ccf491"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        // "foreground": "#B2CCD6"
        // "foreground": "#C3E88D"
        "foreground": "#daf2b6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        // "foreground": "#C792EA"
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#ed8055"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
